#!/bin/zsh

# while brew is not strictly required, it adds a lot of goodies

/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)"

brew tap \
  caskroom/cask \
  caskroom/fonts \
  homebrew/core

brew install \
  mas \
	jq \
  tmux \
  docker \
  gnu-sed \
  wget \
  netcat \
  netcat6 \
  mycli \
  mtr

brew cask install
  caffeine \
  font-hack \
  slack \
  keepassxc
