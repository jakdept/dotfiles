<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.deleteos.tmpwatch-downloads</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/sbin/tmpwatch</string>
        <string>14d</string>
        <string>/Users/$(whoami)/Downloads/</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
          <key>Hour</key>
          <integer>15</integer>
          <key>Minute</key>
          <integer>30</integer>
    </dict>
</dict>
</plist>
